    "version": "0.2.0",
    "configurations": [
        
        {
            "preLaunchTask": "start_foundry", // The name of the task defined above
            "type": "chrome",
            "request": "launch",
            "cleanUp": "wholeBrowser",
            "name": "Launch Foundry Chrome",
            "url": "http://192.168.1.114:30000/setup",
            "webRoot": "${workspaceFolder}",
            "runtimeArgs": [
            "--auto-open-devtools-for-tabs"
        ]
        }
    ]
}