{
    "id": "simpled20",
    "title": "Simple D20",
    "description": "A simple rule system that allows you to play a d20 style rpg without having to be burndened with a specific brand.",
    "version": "0.0.1",
    "compatibility": {
        "minimum": 12,
        "verified": 12
    },
    "authors": [{
        "name": "rwk"
    }],
    "languages": [
      {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
      }
    ],
    "esmodules": [
        "module/simpled20.mjs"
    ],
    "styles": [
        "css/simpled20.css"
    ],
